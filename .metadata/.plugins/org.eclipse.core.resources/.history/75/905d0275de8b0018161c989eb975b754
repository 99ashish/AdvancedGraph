package com.jda.core;

import java.util.Scanner;

class person{
	int age;
	String name;
	static public boolean compare(person a, person b)
	{
		return a.age>b.age;
	}
}
public class GenericSort{
	public static void sort(person[] arr,int len)
	{
		for(int i=0;i<len;i++)
		{
			person key=arr[i];
			int j;
			for( j=i-1; j>=0 && person.compare(arr[j],key) ; j--)
				arr[j+1]=arr[j];
			arr[j+1]=key;
		}
	}
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		person[] arr=new person[3];
		for(int i=0;i<3;i++)
		{
			arr[i].age=sc.nextInt();
			arr[i].name=sc.nextLine();
		}
		sort(arr,3);
		for(int i=0;i<3;i++)
		{
            System.out.println(arr[i].age+ " "+ arr[i].name);
		}
}
}