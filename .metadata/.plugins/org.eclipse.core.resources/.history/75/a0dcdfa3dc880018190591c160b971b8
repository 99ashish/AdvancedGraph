package com.jda.Utility;

import java.util.HashSet;
import java.util.Set;

public class Queue {
	public static Set<Integer> accountNo= new HashSet<>();
	static int id=23123;
	static int size = 0;
	String name;
	int uniqueId;
	public static Queue front;
	public static Queue back;
	Queue link;
	public void push(String val) {
		uniqueId=id;
		accountNo.add(uniqueId);
		id++;
		size++;
		if (front == null) {
			Queue node = new Queue();
			node.name = val;
			front = node;
			back = node;
		} else {
			Queue node = new Queue();
			node.name = val;
			back.link = node;
			back = node;
		}
	}

	public String  deque() {
		size--;
		String ch=front.name;
		front = front.link;
		return ch;
	}

	public boolean isEmpty() {
		if(front==null)
			return true;
		if (size > 0)
			return false;
		return true;
	}
	public int size() {
		return size;
	}

}
