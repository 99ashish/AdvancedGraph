package com.jda.Utility;
import com.jda.Utility.CustomLinkedList;
public class Stack {
	static int size =0;
	char data;
	public static Stack top;
	Stack next;
	Stack prev;
	public void push(char val)
	{
		size++;
		if (top == null) {
			Stack node = new Stack();
			node.data = val;
			top = node;
		} else {
			Stack node = new Stack();
			node.data = val;
			top.next = node;
			node.prev=top;
			top=node;
		}
	}
	public char peek()
	{
		return top.data;
	}
	public void pop()
	{
		top=top.prev;
		top.next=null;
	}
	public boolean isEmpty()
	{
		if(size>0)
			return false;
		return true;
	}
public int sizeOfStack()
{
	return size;
}
}
