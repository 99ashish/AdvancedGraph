package com.jda.core;
import com.jda.Utility.CustomLinkedList;
import com.jda.utility.AlgorithmUtility;
public class QueueLinkedListCalender 
{
	public static void main(String[] args) 
	{
		int month = Integer.valueOf(args[0]);
		int year = Integer.valueOf(args[1]);
		int strtDay = AlgorithmUtility.dayOfWeek(month, 1, year);
		CustomLinkedList cal=new CustomLinkedList();
		cal.add("S ");
		cal.add("M ");
		cal.add("T ");
		cal.add("");
		int[] day = { 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };
		if ((year % 400 == 0) || (year % 4 == 0 && year % 100 != 0))
			day[1]++;
		int totDay = day[month - 1];
		int cntDay = 1;
		for (int i = 1; i < 6; i++) {
			for (int j = 0; j < 7; j++) {
				if (i == 1 && strtDay > j)
					cal[i][j] = "             ";
				else {
					if (cntDay < totDay + strtDay) {
						if (cntDay < 9)
							cal[i][j] = String.valueOf((cntDay++) + "          ");
						else
							cal[i][j] = String.valueOf((cntDay++) + "       ");
					} else
						cal[i][j] = "     ";
				}
	}

}
