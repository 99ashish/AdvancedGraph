package com.jda.Utility;

public class Queue {
	static int size = 0;
	String name;
	public static Queue front;
	public static Queue back;
	Queue link;

	public void push(String val) {
		size++;
		if (front == null) {
			Queue node = new Queue();
			node.name = val;
			front = node;
			back = node;
		} else {
			Queue node = new Queue();
			node.name = val;
			back.link = node;
			back = node;
		}
	}

	public String  deque() {
		size--;
		String ch=front.name;
		front = front.link;
		return ch;
	}

	public boolean isEmpty() {
		if(front==null)
			return true;
		if (size > 0)
			return false;
		return true;
	}

	public int size() {
		return size;
	}

}
